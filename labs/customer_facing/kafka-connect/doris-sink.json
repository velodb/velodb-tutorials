{
    "name": "velodb-kafka-events-sink",
    "config": {
        "connector.class": "org.apache.doris.kafka.connector.DorisSinkConnector",
        "tasks.max": "1",
        "topics": "fact_events",
        "doris.urls": "${VELODB_FE_HOST}",
        "doris.http.port": "${VELODB_HTTP_PORT}",
        "doris.query.port": "9030",
        "doris.user": "${VELODB_USER}",
        "doris.password": "${VELODB_PASSWORD}",
        "doris.database": "${VELODB_DATABASE}",
        "doris.topic2table.map": "fact_events:kafka_fact_events",
        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "value.converter.schemas.enable": "false",
        "buffer.count.records": "1000",
        "buffer.flush.time": "10",
        "buffer.size.bytes": "1000000",
        "doris.request.connect.timeout.ms": "30000",
        "doris.request.read.timeout.ms": "30000"
    }
}
